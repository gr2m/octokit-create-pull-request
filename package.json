{
  "name": "octokit-create-pull-request",
  "version": "0.0.0-development",
  "description": "Octokit plugin to create a pull request with multiple file changes",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=html && open coverage/index.html",
    "lint": "prettier --check '{lib,test}/**/*.{js,json}' README.md package.json",
    "lint:fix": "prettier --write '{lib,test}/**/*.{js,json}' README.md package.json",
    "pretest": "npm run -s lint",
    "test": "tap --100 --coverage test/*-test.js"
  },
  "keywords": [
    "github",
    "octokit",
    "plugin",
    "pull-request"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "MIT",
  "devDependencies": {
    "@octokit/rest": "^16.29.0",
    "prettier": "^1.19.1",
    "semantic-release": "^16.0.0",
    "tap": "^14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gr2m/octokit-create-pull-request.git"
  },
  "bugs": {
    "url": "https://github.com/gr2m/octokit-create-pull-request/issues"
  },
  "homepage": "https://github.com/gr2m/octokit-create-pull-request#readme"
}
